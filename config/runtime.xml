<?xml version="1.0" encoding="UTF-8"?>
<dmz>
<runtime>
   <time>
      <frequency value="60.0"/>
   </time>
   <message name="Update_Tool_Name" monostate="true"/>
   <object-type name="Tool"/>
   <object-type name="Tool Link Node" parent="Tool">
      <canvas-object/>
   </object-type>
   <object-type name="Computer">
      <render/>
      <canvas-object/>
      <canvas-object-basic>
         <items>
            <group name="base node">
               <image name="node" resource="node">
                  <translate x="-10" y="-10"/>
               </image>
            </group>
         </items>
         <switch>
            <state group="base node">
               <item name="node"/>
            </state>
         </switch>
      </canvas-object-basic>
   </object-type>
   <object-type name="Server" parent="Computer">
      <render/>
   </object-type>
   <object-type name="Work Station" parent="Computer">
      <render/>
      <canvas-object/>
      <canvas-object-basic>
         <items>
            <group name="base node">
               <image name="node" resource="workstation">
                  <translate x="-50" y="-50"/>
               </image>
            </group>
         </items>
         <switch>
            <state group="base node">
               <item name="node"/>
            </state>
         </switch>
      </canvas-object-basic>
   </object-type>
   <object-type name="Laptop" parent="Computer">
      <render/>
   </object-type>
   <object-type name="Tablet" parent="Computer">
      <render/>
   </object-type>
