               <image name="node" resource="node">
                  <translate x="-10" y="-10"/>
               </image>
            </group>
         </items>
         <switch>
            <state group="base node">
               <item name="node"/>
            </state>
         </switch>
      </canvas-object-basic>
   </object-type>
